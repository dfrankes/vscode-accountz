{
    "name": "vscode-accountz",
    "displayName": "VScode-Accountz",
    "description": "Multipurpose account manager for visual studio code",
    "version": "0.0.1",
    "publisher": "Davy Frankes",
    "repository": "https://github.com/microsoft/vscode-extension-samples/helloworld-sample",
    "engines": {
      "vscode": "^1.34.0"
    },
    "categories": ["Other"],
    "activationEvents": ["onCommand:extension.helloWorld"],
    "main": "./out/extension.js",
    "contributes": {
      "commands": [
        {
          "command": "extension.helloWorld",
          "title": "Hello World"
        }
      ]
    },
    "scripts": {
      "vscode:prepublish": "npm run compile",
      "compile": "tsc -p ./",
      "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
      "@types/node": "^8.10.25",
      "@types/vscode": "^1.34.0",
      "tslint": "^5.16.0",
      "typescript": "^3.4.5"
    }
  }
  